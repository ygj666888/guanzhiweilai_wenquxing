@import "../../../uvue.wxss";
:host{display:flex;flex-direction:column}

.experience-container {
	flex: 1;
	background-color: #f8f8f8;
	display: flex;
	align-items: center;
	justify-content: center;
}
.content {
	text-align: center;
}
.title {
	font-size: 36rpx;
	font-weight: 600;
	color: #000000;
	margin-bottom: 20rpx;
	display: block;
}
.desc {
	font-size: 28rpx;
	color: #666666;
	margin-bottom: 40rpx;
	display: block;
}
.status {
	font-size: 24rpx;
	color: #999999;
}
